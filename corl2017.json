{
    "model": "Approach",
    "modules": {
        "Encoder"  : "ResNet50",
        "Decoder"  : "TVADecoder",
        "Attention": "Atten11",
        "Control"  : "SeqModule"
    },
    "sampling": {
        "alpha": 1.0,
        "beta" : 1.0,
        "betaLinear": false,
        "betaPhase": 50,
        "balance": true,
        "c": 1.4
    },
    "general": {
        "dataset"  : "CoRL2017",
        "validPath": "./data/h5file/SeqVal/",
        "trainPath": "./data/h5file/SeqTrain/",
        "savedPath": "/media/victor/Datos/Saved/"
    },
    "train": {
        "scheduler": {
            "learning_rate_decay_factor": 0.2,
            "learning_rate_decay_steps" : 80
        },
        "optimizer": {
            "type": "Adam",
            "beta1": 0.70,
            "beta2": 0.85,
            "learningRate": 0.0001
        }
    }
}