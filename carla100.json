{
    "model": "Approach",
    "modules": {
        "Encoder"  : "ResNet50",
        "Decoder"  : "TVADecoder",
        "Attention": "Atten11",
        "Control"  : "SeqModule"
    },
    "sampling": {
        "alpha": 1.0,
        "beta" : 1.0,
        "betaLinear": false,
        "betaPhase": 50,
        "balance": true,
        "c": 1.4
    },
    "general": {
        "stepView":   1,
        "dataset"  : "CARLA100",
        "validPath": "./data/h5file/C100train/",
        "trainPath": "./data/h5file/C100val/",
        "savedPath": "/opt/MrCat/Saved/"
    },
    "train": {
        "scheduler": {
            "learning_rate_decay_factor": 0.1,
            "learning_rate_decay_steps" : 50
        },
        "optimizer": {
            "type": "Adam",
            "beta1": 0.70,
            "beta2": 0.85,
            "learningRate": 0.00001
        },
        "loss": {
            "type": "WeightedMultiTask",
            "lambda_gas"  : 0.33333,
            "lambda_steer": 0.26667,
            "lambda_brake": 0.06667,
            "lambda_desc" : 0.33333,
            "lambda_speed" : 0.05,
            "lambda_action": 0.95
        }
    }
}